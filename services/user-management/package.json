{
  "name": "user-management",
  "version": "1.0.0",
  "description": "User authentication, roles, and profiles service",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write 'src/**/*.ts'"
  },
  "dependencies": {
    "@grpc/proto-loader": "*",
    "@opentelemetry/sdk-node": "*",
    "@sentry/node": "*",
    "@upstash/redis": "*",
    "bcryptjs": "*",
    "cors": "*",
    "dotenv": "*",
    "drizzle-orm": "*",
    "drizzle-zod": "*",
    "express": "*",
    "grpc": "*",
    "helmet": "*",
    "jsonwebtoken": "*",
    "nats": "*",
    "pg": "*",
    "postgres": "^3.4.5",
    "swagger-ui-express": "*",
    "zod": "*"
  },
  "devDependencies": {
    "@types/bcryptjs": "*",
    "@types/cors": "*",
    "@types/express": "*",
    "@types/jsonwebtoken": "*",
    "@types/node": "*",
    "@types/pg": "*",
    "@types/swagger-ui-express": "*",
    "@typescript-eslint/eslint-plugin": "*",
    "@typescript-eslint/parser": "*",
    "@vitest/coverage-v8": "*",
    "drizzle-kit": "*",
    "eslint": "*",
    "prettier": "*",
    "ts-node-dev": "*",
    "typescript": "*",
    "vitest": "*"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
