{
  "name": "user-management",
  "version": "1.0.0",
  "description": "User authentication, roles, and profiles service",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write 'src/**/*.ts'"
  },
  "dependencies": {
    "@grpc/proto-loader": "^0.7.10",
    "@opentelemetry/sdk-node": "^0.45.1",
    "@sentry/node": "^7.64.0",
    "@upstash/redis": "^1.22.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.28.5",
    "drizzle-zod": "^0.5.1",
    "express": "^4.18.2",
    "grpc": "^1.24.11",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.1",
    "nats": "^2.16.0",
    "pg": "^8.11.3",
    "swagger-ui-express": "^5.0.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.5.7",
    "@types/pg": "^8.10.2",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "@vitest/coverage-v8": "^0.34.3",
    "drizzle-kit": "^0.19.13",
    "eslint": "^8.48.0",
    "prettier": "^3.0.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}