# Server settings
PORT=3002
NODE_ENV=development

# Upstash Redis - Please fill in your credentials
UPSTASH_REDIS_REST_URL=
UPSTASH_REDIS_REST_TOKEN=

# Supabase PostgreSQL Database - Please fill in your DIRECT_URL
# The service prefers DIRECT_URL over DATABASE_URL
DIRECT_URL=

# JWT settings (should be consistent with user-management service)
JWT_SECRET=your-super-secret-key-for-enrollment
JWT_EXPIRES_IN=1d
